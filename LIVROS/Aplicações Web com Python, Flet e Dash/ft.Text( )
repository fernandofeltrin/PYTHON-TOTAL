import flet as ft

def main(page: ft.Page):
    titulo1 = ft.Text('Cadastro de Paciente')
    titulo2 = ft.Text(value = 'Cadastro de Paciente',
                  color = 'blue',
                  size = 18,
                  weight = 'bold',
                  font_family = 'Open Sans')
    
    page.add(titulo1, titulo2)

ft.app(target = main)
