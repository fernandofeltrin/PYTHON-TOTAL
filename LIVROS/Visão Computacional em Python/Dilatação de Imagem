# Livro VISÃO COMPUTACIONAL EM PYTHON - FERNANDO FELTRIN https://www.amazon.com.br/dp/B08NTW8TNV

import cv2
import numpy as np
import matplotlib.pyplot as plt

imagem8 = cv2.imread('placa.jpg', 1)

elem_elip = cv2.getStructuringElement(cv2.MORPH_ELLIPSE,
                                      (7,7))

imgprocessada2 = cv2.dilate(imagem8,
                            elem_elip,
                            iterations = 1)

fig = plt.figure(figsize=(10,8))
ax1 = fig.add_subplot(121)
plt.imshow(imagem8)
ax2 = fig.add_subplot(122)

plt.imshow(imgprocessada2)
plt.show()

