imagem6 = io.imread('pato.jpg')

filtro = np.vstack([np.hstack([imagem6, cv2.blur(imagem6, (3, 3))]),
                    np.hstack([cv2.blur(imagem6, (5,5)), cv2.blur(imagem6, (7,7))]),
                    np.hstack([cv2.blur(imagem6, (9,9)), cv2.blur(imagem6, (11,11))]),
                    ])

fig = plt.figure(figsize=(8,5))
plt.imshow(imagem6)

